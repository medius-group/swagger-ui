<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="static" stopProcessing="true">  
					<match url="(.*)(.css|.js|.png)" />
					<action type="Rewrite" url="dist/{R:0}{R:1}" />
				</rule>
				<rule name="Proxy" stopProcessing="true">  
					<match url=".*" />
					<action type="Rewrite" url="/dist/index.html" />
				</rule>
        	</rules>
        </rewrite>
    </system.webServer>
</configuration>